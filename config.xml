<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.cats" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PawsomeCats</name>
    <preference name="DisallowOverscroll" value="true" />
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon height="180" src="res/icon/ios/icon.png" width="180" />
        <icon height="60" src="res/icon/ios/icon.png" width="60" />
        <icon height="120" src="res/icon/ios/icon.png" width="120" />
        <icon height="76" src="res/icon/ios/icon.png" width="76" />
        <icon height="152" src="res/icon/ios/icon.png" width="152" />
        <icon height="40" src="res/icon/ios/icon.png" width="40" />
        <icon height="80" src="res/icon/ios/icon.png" width="80" />
        <icon height="57" src="res/icon/ios/icon.png" width="57" />
        <icon height="114" src="res/icon/ios/icon.png" width="114" />
        <icon height="72" src="res/icon/ios/icon.png" width="72" />
        <icon height="144" src="res/icon/ios/icon.png" width="144" />
        <icon height="29" src="res/icon/ios/icon.png" width="29" />
        <icon height="58" src="res/icon/ios/icon.png" width="58" />
        <icon height="50" src="res/icon/ios/icon.png" width="50" />
        <icon height="100" src="res/icon/ios/icon.png" width="100" />
        <icon height="167" src="res/icon/ios/icon.png" width="167" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="60" src="www/resources/icon.png" width="60" />
        <icon density="mdpi" height="57" platform="ios" src="res/icon/ios/icon.png" width="57" />
    </platform>
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-console" spec="^1.0.7" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
